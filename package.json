{
	"name": "site_test",
	"private": true,
	"version": "0.0.1",
	"homepage": "https://lalunainsky.github.io/site_test/",
	"type": "module",
	"scripts": {
		"predeploy": "npm run build",
		"deploy": "gh-pages -d dist",
